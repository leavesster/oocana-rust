{
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rust-lang.rust-analyzer",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.addMissingImports": "explicit"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "conventionalCommits.scopes": [
    "cli",
    "core",
    "examples",
    "mainframe",
    "manifest_reader",
    "sdk",
    "tests",
    "utils",
    "deployment",
    "env",
    "i18n",
    "version",
    "scripts",
    "readme",
    "project",
    "block_meta",
    "runtime",
    "manifest_meta"
  ],
}
